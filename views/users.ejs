<!DOCTYPE html>
<html>

<head>
</head>

<body>
    <h1>User List</h1>
    <table border="1">
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(user=> { %>
                <tr>
                    <td>
                        <%= user.name %>
                    </td>
                    <td>
                        <%= user.email %>
                    </td>
                </tr>
                <% }) %>
        </tbody>
    </table>
</body>

</html>